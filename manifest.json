{
  "manifest_version": 2,

  "name": "jddbd",
  "description": "京东夺宝岛秒杀神器,给最2的你...",
  "version": "0.1",

  "icons": {"48": "icons/logo_avatar.png" },

  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["jquery-2.1.3.min.js", "jd.js"]
  }],

  "permissions": [
    "tabs",
    "activeTab"
  ]
}

